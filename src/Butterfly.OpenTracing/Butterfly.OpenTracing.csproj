<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\build\common.props" />
  <PropertyGroup>
    <Description>Reference implementation of OpenTracing API</Description>
    <AssemblyTitle>Butterfly.OpenTracing</AssemblyTitle>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <AssemblyName>Housecool.Butterfly.OpenTracing</AssemblyName>
    <PackageId>Housecool.Butterfly.OpenTracing</PackageId>
    <PackageTags>OpenTracing;Tracing</PackageTags>
    <PackageReleaseNotes>
    </PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseUrl>https://github.com/nishizhen/butterfly-csharp/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/nishizhen/butterfly-csharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/nishizhen/butterfly-csharp</RepositoryUrl>
    <RootNamespace>Housecool.Butterfly.OpenTracing</RootNamespace>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|netstandard2.0|AnyCPU'">
    <DefineConstants>TRACE;DEBUG;NETSTANDARD2_0</DefineConstants>
    <LangVersion>default</LangVersion>
  </PropertyGroup>
</Project>